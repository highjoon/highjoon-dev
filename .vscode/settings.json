  "cSpell.words": [
    "Adsense",
    "appleboy",
    "autoincrement",
    "awscli",
    "Bezier",
    "cmdk",
    "Combobox",
    "cuid",
    "Embla",
    "envalid",
    "EXTINF",
    "EXTM",
    "gtag",
    "highjoon",
    "highlightjs",
    "hljs",
    "mantine",
    "Mbps",
    "MBTI",
    "Plug'n'Play",
    "precomposed",
    "pretendard",
    "referencetypes",
    "rehype",
    "showkit",
    "splitted",
    "SSOT",
    "supabase",
    "tailwindcss",
    "TARGETDURATION",
